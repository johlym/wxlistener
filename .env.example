# wxlistener Environment Configuration
# Copy this file to .env and update with your values

# Weather station IP address (REQUIRED)
WXLISTENER_IP=10.31.100.42

# Weather station port (default: 45000)
WXLISTENER_PORT=45000

# Polling interval in seconds (default: 60)
WXLISTENER_INTERVAL=60

# Output format: text or json (default: text)
WXLISTENER_FORMAT=text

# Rust logging level: error, warn, info, debug, trace (default: info)
RUST_LOG=info
